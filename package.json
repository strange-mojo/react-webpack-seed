{
  "name": "react-webpack-seed",
  "version": "1.0.0",
  "description": "Base project using react and redux with es6 and webpack.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js --inline --progress --port 8080",
    "prod": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --json > stats.json",
    "test": "test",
    "bundle-analysis": "webpack-bundle-analyzer stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strange-mojo/react-webpack-seed.git"
  },
  "keywords": [
    "react",
    "webpack",
    "seed"
  ],
  "author": "Nick Phillips",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strange-mojo/react-webpack-seed/issues"
  },
  "homepage": "https://github.com/strange-mojo/react-webpack-seed#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.0",
    "rxjs": "^5.4.3",
    "moment": "^2.19.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-minify-webpack-plugin": "^0.2.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  }
